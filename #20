bool isValid(char * s){
     if (s == NULL || s[0] == '\0') return 1;
    char *stack = (char*)malloc(strlen(s)+1); 
    int top =0;
    for (int i = 0; s[i]; i++) {
        if (s[i] == '(' || s[i] == '[' || s[i] == '{') stack[++top] = s[i];       
        else if((s[i] == (stack[top] + 1))||(s[i] == (stack[top] + 2))) top--;        
        else return 0;
    }
    free(stack);
    return (!top);
}
